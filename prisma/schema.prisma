generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id        Int      @id @default(autoincrement()) @map("id")
  username  String   @db.VarChar(50)
  password  String   @db.VarChar(255)
  email     String   @unique @db.VarChar(255)
  role      String   @default("user") @db.VarChar(5)
  createdat DateTime @default(now()) @db.Timestamp(6)
  updatedat DateTime @default(now()) @db.Timestamp(6)
  rating    rating[]
}

model articles {
  id          BigInt    @id(map: "articles_pkey1") @default(autoincrement())
  title       String?
  category    Category?
  description String?
  location    String?
  image       String?
  createdat   DateTime? @default(now()) @db.Timestamptz(6)
  updatedat   DateTime? @default(now()) @db.Timestamptz(6)
}

model rating {
  id        BigInt    @id @default(autoincrement())
  rating    Rating?
  ulasan    String?
  createdAt DateTime? @default(now()) @db.Timestamp(6)
  userid    Int?
  users     users?    @relation(fields: [userid], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_user")
}

enum Category {
  Wisata
  Budaya
}

enum Rating {
  ONE   @map("1")
  TWO   @map("2")
  THREE @map("3")
  FOUR  @map("4")
  FIVE  @map("5")
}

enum enum_rating_rating {
  ONE   @map("1")
  TWO   @map("2")
  THREE @map("3")
  FOUR  @map("4")
  FIVE  @map("5")
}
