{
    "version": 2,
    "builds": [
      {
        "src": "prisma/schema.prisma",
        "use": "@prisma/cli",
        "config": { "packageManager": "yarn" }
      },
      {
        "src": "src/index.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/index.js"
      }
    ]
  }
  